<div id="dashboard">
	<div class="loading">
		<h3>Tracking Your Location...</h3>
	</div>
	<div class="nogeo">
		<h3>Blasphemy. You're no fun.</h3>
	</div>
	<div class="main">
		<%= render :partial => 'shared/card', :locals => {user: @user} %>
		<div class="presser">
			<%= image_tag asset_path('handAnimation.png') %>
			<div></div>
		</div>
	</div>
	<div class="edit">
	
	</div>
	<div class="results">
		<div class="card-reference card page-curl curl">
			<div class="top">
				<div class="photo">
					<img class="profile-photo">
				</div>
				<div class="header">
					<h1 class="name"></h1>
					<h1 class="lastname"></h1>
				</div>
			</div>
			<div class="bottom shadow-bottom">
				<ul class="fields">
					<li class="card-field field-reference">
						<img class="field-icon">
						<span class="field-text">
							<a href="#"></a>
						</span>
					</li>
				</ul>
			</div>
		</div>
	</div>
</div>

<script type="text/javascript">
Dashboard.init();
</script>